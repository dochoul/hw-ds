{
  "name": "gutils",
  "version": "1.2.6",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./constant": {
      "types": "./dist/constant/index.d.ts",
      "import": "./dist/constant/index.js",
      "require": "./dist/constant/index.cjs"
    },
    "./cookie": {
      "types": "./dist/cookie/index.d.ts",
      "import": "./dist/cookie/index.js",
      "require": "./dist/cookie/index.cjs"
    },
    "./date": {
      "types": "./dist/date/index.d.ts",
      "import": "./dist/date/index.js",
      "require": "./dist/date/index.cjs"
    },
    "./file": {
      "types": "./dist/file/index.d.ts",
      "import": "./dist/file/index.js",
      "require": "./dist/file/index.cjs"
    },
    "./queryString": {
      "types": "./dist/queryString/index.d.ts",
      "import": "./dist/queryString/index.js",
      "require": "./dist/queryString/index.cjs"
    },
    "./secure": {
      "types": "./dist/secure/index.d.ts",
      "import": "./dist/secure/index.js",
      "require": "./dist/secure/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js",
      "require": "./dist/validators/index.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "cross-env yarn clean && yarn build:tsc && yarn build:js",
    "build:tsc": "cross-env yarn tsc --emitDeclarationOnly",
    "build:js": "cross-env node build.mjs",
    "clean": "cross-env rimraf /packages/dist",
    "docs": "cross-env gitbook serve",
    "test": "cross-env jest",
    "format": "cross-env prettier --write \"**/*.{js,ts}\"",
    "lint": "cross-env eslint --ignore-path .eslintignore --ext .ts,.tsx .",
    "lint --fix": "cross-env eslint --ignore-path .eslintignore --ext .ts,.tsx .",
    "commit": "cross-env ./node_modules/cz-customizable/standalone.js"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/jest": "^29.5.2",
    "@types/jest-plugin-context": "^2.9.5",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "esbuild": "latest",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "fs": "^0.0.1-security",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-plugin-context": "^2.9.0",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2"
  },
  "config": {
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "publishConfig": {
    "registry": "http://repo.gabia.com/repository/npm-repository/"
  }
}
